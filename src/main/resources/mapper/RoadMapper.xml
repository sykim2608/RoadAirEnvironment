<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ntels.sykim.RoadAirEnvironment.dao.RoadMapper">

    <select id="getCategory" resultType="String">
          SELECT
        DISTINCT  CATEGORY
            FROM  ROAD_CATEGORY
    </select>

    <select id="getRoadName" parameterType="String" resultType="java.util.List">
          SELECT
            NAME
            FROM  ROAD_CATEGORY
           WHERE
        CATEGORY  =  #{category}
    </select>

    <insert id="addCategory" parameterType="com.ntels.sykim.RoadAirEnvironment.model.RoadCategory">
        INSERT
          INTO  ROAD_CATEGORY (
                CATEGORY
                , NAME )
        VALUES  (
                #{category}
                , #{name} )
    </insert>
</mapper>